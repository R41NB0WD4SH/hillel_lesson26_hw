

<h1>Contacts</h1>

@model ContactsModel

    <div class="contacts">
        <ul>
            <li>
                Phone Number: @Model.PhoneNumber
            </li>
            <li>
                Email: @Model.Email
            </li>
            <li>
                Telegram: @Model.Telegram
            </li>
            <li>
                LinkedIn: <a href="@Model.Linkedin">@Model.Linkedin</a>
            </li>
        </ul>
    </div>

